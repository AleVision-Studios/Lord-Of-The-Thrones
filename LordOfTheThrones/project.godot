; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="LordOfTheThrones"
run/main_scene="res://Scenes/game.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
config/icon="res://Art/icon.svg"

[autoload]

PlayerState="*res://Script/PlayerState.cs"
SceneLoader="*res://Scenes/scene_loader.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[dotnet]

project/assembly_name="LordOfTheThrones"

[file_customization]

folder_colors={
"res://Art/": "green",
"res://NPC/": "purple",
"res://Scenes/": "yellow",
"res://Script/": "red"
}

[gui]

theme/custom_font="res://Art/Fonts/alagard.ttf"
